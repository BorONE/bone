#!/bin/bash

function print_help {
  echo -e "Usage: bone COMMAND [ARG]..."
  echo -e 'Run command'
  echo

  echo -e 'Commands:'
  echo -e ' create'
  echo -e ' run'
  echo -e ' green'
  echo -e ' test'
}

# handle empty
if [[ $# -eq 0 ]]; then
  print_help
  exit 0
fi

# hardcode is bad, but whatever
/acos/bin/sub/$@
